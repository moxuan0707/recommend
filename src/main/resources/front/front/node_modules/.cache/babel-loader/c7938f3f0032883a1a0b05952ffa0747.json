his.ruleForm.crossrefid=crossrefid;\r\n                 var params = {\r\n                     page: 1,\r\n                     limit: 10,\r\n                     crossuserid:crossuserid,\r\n                     crossrefid:crossrefid,\r\n                 }\r\n                 this.$http.get('yonghu/list', {\r\n                  params: params\r\n                 }).then(res => {\r\n                     if(res.data.data.total>=crossoptnum) {\r\n                         this.$message({\r\n                          message: localStorage.getItem('tips'),\r\n                          type: 'success',\r\n                          duration: 1500,\r\n                         });\r\n                          return false;\r\n                     } else {\r\n                         // 跨表计算\r\n\r\n\r\n                          this.$http.post('yonghu/add', this.ruleForm).then(res => {\r\n                              if (res.data.code == 0) {\r\n                                  this.$message({\r\n                                      message: '操作成功',\r\n                                      type: 'success',\r\n                                      duration: 1500,\r\n                                      onClose: () => {\r\n                                          this.$router.go(-1);\r\n                                      }\r\n                                  });\r\n                              } else {\r\n                                  this.$message({\r\n                                      message: res.data.msg,\r\n                                      type: 'error',\r\n                                      duration: 1500\r\n                                  });\r\n                              }\r\n                          });\r\n                     }\r\n                 });\r\n             } else {\r\n\r\n\r\n                  this.$http.post('yonghu/add', this.ruleForm).then(res => {\r\n                     if (res.data.code == 0) {\r\n                       