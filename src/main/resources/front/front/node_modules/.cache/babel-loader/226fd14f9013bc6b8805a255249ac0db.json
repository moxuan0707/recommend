orm-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}'  label=\"公告类型\" prop=\"gonggaoleixing\">\r\n            <el-select v-model=\"ruleForm.gonggaoleixing\" placeholder=\"请选择公告类型\" >\r\n              <el-option\r\n                  v-for=\"(item,index) in gonggaoleixingOptions\"\r\n                  :key=\"index\"\r\n                  :label=\"item\"\r\n                  :value=\"item\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"公告图片\" prop=\"gonggaotupian\">\r\n            <file-upload\r\n            tip=\"点击上传公告图片\"\r\n            action=\"file/upload\"\r\n            :limit=\"3\"\r\n            :multiple=\"true\"\r\n            :fileUrls=\"ruleForm.gonggaotupian?ruleForm.gonggaotupian:''\"\r\n            @change=\"gonggaotupianUploadChange\"\r\n            ></file-upload>\r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"发布日期\" prop=\"faburiqi\" >\r\n              <el-date-picker\r\n                  format=\"yyyy 年 MM 月 dd 日\"\r\n                  value-format=\"yyyy-MM-dd\"\r\n                  v-model=\"ruleForm.faburiqi\" \r\n                  type=\"date\"\r\n                  placeholder=\"发布日期\">\r\n              </el-date-picker> \r\n          </el-form-item>\r\n          <el-form-item :style='{\"border\":\"0px solid #dfdfdf\",\"padding\":\"10px\",\"boxShadow\":\"0px 0px 0px #eee\",\"margin\":\"0 0 8px 0\",\"borderRadius\":\"8px\",\"background\":\"none\"}' label=\"公告内容\" prop=\"gonggaoneirong\">\r\n            <editor \r\n                :style='{\"minHeight\":\"300px\",\"padding\":\"0\",\"margin\":\"0\",\"borderColor\":\"#ccc\",\"backgroundColor\":\"#fff\",\"borderRadius\":\"0\",\"borderWidth\":\"0\",\"width\":\"100%\",\"borderStyle\":\"solid\",\"height\":\"auto\"}'\r\n                v-model=\"ruleForm.gonggaoneirong\" \r\n                class=\"editor\" \r\n                action=\"file/upload\">\r\n            </editor>\r\n          </el-form-item>\r\n\r\n      <el-form-item :style='{\"padding\":\"0\",\"margin\":\"20px 0 20px 0\"}'>\r\n        <el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0\",\"boxShadow\":\"1px 2px 3px #aaa\",\"margin\":\"0 20px 0 0\",\"color\":\"#fff\",\"outline\":\"none\",\"borderRadius\":\"4px\",\"background\":\"linear-gradient(180deg, rgba(17,112,210,1) 0%, rgba(8,179,68,1) 100%)\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}'  type=\"primary\" @click=\"onSubmit\">提交</el-button>\r\n        <el-button :style='{\"border\":\"1px solid #bbb\",\"cursor\":\"pointer\",\"padding\":\"0\",\"boxShadow\":\"1px 2px 6px #ccc\",\"margin\":\"0\",\"color\":\"#999\",\"outline\":\"none\",\"borderRadius\":\"4px\",\"background\":\"#fff\",\"width\":\"128px\",\"lineHeight\":\"40px\",\"fontSize\":\"14px\",\"height\":\"40px\"}' @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        id: '',\r\n        type: '',\r\n        userTableName: localStorage.getItem('UserTableName'),\r\n        ruleForm: {\r\n          gonggaobiaoti: '',\r\n          gonggaoleixing: '',\r\n          gonggaotupian: '',\r\n          gonggaoneirong: '',\r\n          faburiqi: '',\r\n        },\r\n        gonggaoleixingOptions: [],\r\n        rules: {\r\n          gonggaobiaoti: [\r\n            { required: true, message: '公告标题不能为空', trigger: 'blur' },\r\n          ],\r\n          gonggaoleixing: [\r\n            { required: true, message: '公告类型不能为空', trigger: 'blur' },\r\n          ],\r\n          gonggaotupian: [\r\n          ],\r\n          gonggaoneirong: [\r\n          ],\r\n          faburiqi: [\r\n          ],\r\n        },\r\n      };\r\n    },\r\n    computed: {\r\n\r\n\r\n\r\n    },\r\n    created() {\r\n\t  //this.bg();\r\n      let type = this.$route.query.type ? this.$route.query.type : '';\r\n      this.init(type);\r\n      this.ruleForm.faburiqi = this.getCurDate()\r\n    },\r\n    methods: {\r\n      getMakeZero(s) {\r\n          return s < 10 ? '0' + s : s;\r\n      },\r\n      // 下载\r\n      download(file){\r\n        window.open(`${file}`)\r\n      },\r\n      // 初始化\r\n      init(type) {\r\n        this.type = type;\r\n        if(type=='cross'){\r\n          var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n          for (var o in obj){\r\n            if(o=='gonggaobiaoti'){\r\n              this.ruleForm.gonggaobiaoti = obj[o];\r\n              continue;\r\n            }\r\n            if(o=='gonggaoleixing'){\r\n              this.ruleForm.gonggaoleixing = obj[o];\r\n              continue;\r\n            }\r\n            if(o=='gonggaotupian'){\r\n              this.ruleForm.gonggaotupian = obj[o].split(\",\")[0];\r\n              continue;\r\n            }\r\n            if(o=='gonggaoneirong'){\r\n              this.ruleForm.gonggaoneirong = obj[o];\r\n              continue;\r\n            }\r\n            if(o=='faburiqi'){\r\n              this.ruleForm.faburiqi = obj[o];\r\n              continue;\r\n            }\r\n          }\r\n        }\r\n        // 获取用户信息\r\n        this.$http.get(this.userTableName + '/session', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            var json = res.data.data;\r\n          }\r\n        });\r\n        this.gonggaoleixingOptions = \"资讯,通知,公告,其它\".split(',')\r\n      },\r\n\r\n    // 多级联动参数\r\n      // 多级联动参数\r\n      info(id) {\r\n        this.$http.get('gonggaoxinxi/detail/${id}', {emulateJSON: true}).then(res => {\r\n          if (res.data.code == 0) {\r\n            this.ruleForm = res.data.data;\r\n          }\r\n        });\r\n      },\r\n      // 提交\r\n      onSubmit() {\r\n\r\n        //更新跨表属性\r\n        var crossuserid;\r\n        var crossrefid;\r\n        var crossoptnum;\r\n        this.$refs[\"ruleForm\"].validate(valid => {\r\n          if(valid) {\r\n            if(this.type=='cross'){\r\n                 var statusColumnName = localStorage.getItem('statusColumnName');\r\n                 var statusColumnValue = localStorage.getItem('statusColumnValue');\r\n                 if(statusColumnName && statusColumnName!='') {\r\n                     var obj = JSON.parse(localStorage.getItem('crossObj'));\r\n                     if(!statusColumnName.startsWith(\"[\")) {\r\n                         for (var o in obj){\r\n                             if(o==statusColumnName){\r\n                                 obj[o] = statusColumnValue;\r\n                             }\r\n                         }\r\n                         var table = localStorage.getItem('crossTable');\r\n                         this.$http.post(table+'/update', obj).then(res => {});\r\n                     } else {\r\n                            crossuserid=Number(localStorage.getItem('userid'));\r\n                            crossrefid=obj['id'];\r\n                            crossoptnum=localStorage.getItem('statusColumnName');\r\n                            crossoptnum=crossoptnum.replace(/\\[/,\"\").replace(/\\]/,\"\");\r\n                     }\r\n                 }\r\n            }\r\n            if(crossrefid && crossuserid) {\r\n                 this.ruleForm.crossuserid=crossuserid;\r\n                 this.ruleForm.crossrefid=crossrefid;\r\n                 var params = {\r\n                     page: 1,\r\n                     limit: 10,\r\n                     crossuserid:crossuserid,\r\n                     cr