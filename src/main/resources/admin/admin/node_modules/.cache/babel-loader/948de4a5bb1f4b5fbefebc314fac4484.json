 D = date.getDate() < 10 ? '0' + date.getDate() : date.getDate()\r\n\t  \r\n\t  const H = date.getHours() < 10 ? '0' + date.getHours() : date.getHours() // 小时\r\n\t  const I = date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes() // 分钟\r\n\t  const S = date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds() // 秒\r\n\t  \r\n\t  return `${Y}-${M}-${D} ${H}:${I}:${S}`\r\n\t},\r\n    getSystemIntroduction() {\n        this.$http({\r\n            url: `systemintro/detail/1`,\r\n            method: \"get\"\r\n        }).then(({\r\n            data\r\n        }) => {\r\n            if (data && data.code == 0) {\r\n                this.systemIntroductionDetail = data.data\n            }\r\n        })\r\n    },\n    init(){\r\n        if(this.$storage.get('Token')){\r\n        this.$http({\r\n            url: `${this.$storage.get('sessionTable')}/session`,\r\n            method: \"get\"\r\n        }).then(({ data }) => {\r\n            if (data && data.code != 0) {\r\n            router.push({ name: 'login' })\r\n            }\r\n        });\r\n        }else{\r\n            router.push({ name: 'login' })\r\n        }\r\n        this.getSystemIntroduction();\n    },\r\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n    getuserCount() {\r\n        this.$http({\r\n            url: `user/count`,\r\n            method: \"get\"\r\n        }).then(({\r\n            data\r\n        }) => {\r\n            if (data && data.code == 0) {\r\n                this.userCount = data.data\r\n            }\r\n        })\r\n    },\r\n\n//统计接口1\n    userChat3() {\r\n      this.$nextTick(()=>{\r\n\n        var userChart3 = echarts.init(document.getElementById(\"userChart3\"),'macarons');\r\n        this.$http({\r\n            url: \"user/typeStat/xingbie\",\n            method: \"get\",\r\n        }).then(({ data }) => {\r\n            if (data && data.code === 0) {\r\n                let res = data.data;\r\n                let xAxis = [];\n                let yAxis = [];\n                let pArray = []\n                for(let i=0;i<res.length;i++){\n                    xAxis.push(res[i].xingbie);\n                    yAxis.push(parseFloat((res[i].total)));\n                    pArray.push({\n                        value: parseFloat((res[i].total)),\n                        name: res[i].xingbie\n                    })\n                }\n                var option = {};\r\n\t\t\t\tlet titleObj = this.pie.title\r\n\t\t\t\ttitleObj.text = '性别统计'\r\n\t\t\t\t\r\n\t\t\t\tconst legendObj = this.pie.legend\r\n\t\t\t\t\r\n\t\t\t\tlet seriesObj = {\r\n\t\t\t\t\ttype: 'pie',\r\n\t\t\t\t\tradius: '55%',\r\n\t\t\t\t\tcenter: ['50%', '60%'],\r\n\t\t\t\t\tdata: pArray,\r\n\t\t\t\t\temphasis: {\r\n\t\t\t\t\t\titemStyle: {\r\n\t\t\t\t\t\t\tshadowBlur: 10,\r\n\t\t\t\t\t\t\tshadowOffsetX: 0,\r\n\t\t\t\t\t\t\tshadowColor: 'rgba(0, 0, 0, 0.5)'\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t}\r\n\t\t\t\t}\r\n\t\t\t\tseriesObj = Object.assign(seriesObj , this.pie.series)\r\n\t\t\t\t\n                option = {\r\n\t\t\t\t\tbackgroundColor: this.pie.backgroundColor,\r\n\t\t\t\t\tcolor: this.pie.color,\r\n\t\t\t\t\ttitle: titleObj,\r\n\t\t\t\t\tlegend: legendObj,\r\n\t\t\t\t\ttooltip: {\r\n\t\t\t\t\t\ttrigger: 'item',\r\n\t\t\t\t\t\tformatter: '{b} : {c} ({d}%)'\r\n\t\t\t\t\t},\r\n\t\t\t\t\tseries: [seriesObj]\r\n                };\r\n                // 使用刚指定的配置项和数据显示图表。\r\n                userChart3.setOption(option);\r\n                  //根据窗口的大小变动图表\r\n                window.onresize = function() {\r\n                    userChart3.resize();\r\n                };\r\n            }\r\n        });\r\n      })\r\n    },\r\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n//统计接口1\n    shangpinxinxiChat5() {\r\n      this.$nextTick(()=>{\r\n        // shangpinmingcheng shangpinmingcheng\r\n        //  shangpinjiage\r\n\n        var shangpinxinxiChart5 = echarts.init(document.getElementById(\"shangpinxinxiChart5\"),'mint');\r\n        this.$http({\r\n            url: `shangpinx