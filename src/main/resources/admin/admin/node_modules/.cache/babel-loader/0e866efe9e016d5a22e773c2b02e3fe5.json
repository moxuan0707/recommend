rt VueRouter from 'vue-router'\r\nVue.use(VueRouter);\r\n//1.创建组件\r\nimport Index from '@/views/index'\r\nimport Home from '@/views/home'\r\nimport Board from '@/views/board'\r\nimport Login from '@/views/login'\r\nimport NotFound from '@/views/404'\r\nimport UpdatePassword from '@/views/update-password'\r\nimport pay from '@/views/pay'\r\nimport register from '@/views/register'\r\nimport center from '@/views/center'\r\nimport adminexam from '@/views/modules/exampaperlist/exam'\n    import examfailrecord from '@/views/modules/examfailrecord/list'\r\n    import shangpinrukujilu from '@/views/modules/shangpinrukujilu/list'\r\n    import examquestion from '@/views/modules/examquestion/list'\r\n    import shangpinxinxi from '@/views/modules/shangpinxinxi/list'\r\n    import auctionshangpinxinxi from '@/views/modules/auctionshangpinxinxi/list'\r\n    import storeup from '@/views/modules/storeup/list'\r\n    import shangjia from '@/views/modules/shangjia/list'\r\n    import discussyonghushangpin from '@/views/modules/discussyonghushangpin/list'\r\n    import systemintro from '@/views/modules/systemintro/list'\r\n    import exampaperlist from '@/views/modules/exampaperlist/list'\r\n    import examrecord from '@/views/modules/examrecord/list'\r\n    import weixiupingjia from '@/views/modules/weixiupingjia/list'\r\n    import news from '@/views/modules/news/list'\r\n    import yonghushangpin from '@/views/modules/yonghushangpin/list'\r\n    import aboutus from '@/views/modules/aboutus/list'\r\n    import pingjiaxinxi from '@/views/modules/pingjiaxinxi/list'\r\n    import pynews from '@/views/modules/pynews/list'\r\n    import exampaper from '@/views/modules/exampaper/list'\r\n    import menu from '@/views/modules/menu/list'\r\n    import users from '@/views/modules/users/list'\r\n    import dingdanjilu from '@/views/modules/dingdanjilu/list'\r\n    import kefangpingjia from '@/views/modules/kefangpingjia/list'\r\n    import shangpinfenlei from '@/views/modules/shangpinfenlei/list'\r\n    import chat from '@/views/modules/chat/list'\r\n    import discussshangpinxinxi from '@/views/modules/discussshangpinxinxi/list'\r\n    import messages from '@/views/modules/messages/list'\r\n    import orders from '@/views/modules/orders/list'\r\n    import fahuojilu from '@/views/modules/fahuojilu/list'\r\n    import user from '@/views/modules/user/list'\r\n    import shangpinchukujilu from '@/views/modules/shangpinchukujilu/list'\r\n\n\r\n//2.配置路由   注意：名字\r\nconst routes = [{\r\n    path: '/index',\r\n    name: '首页',\r\n    component: Index,\r\n    children: [{\r\n      // 这里不设置值，是把main作为默认页面\r\n      path: '/',\r\n      name: '首页',\r\n      component: Home,\n      meta: {icon:'', title:'center'}\r\n    }, {\r\n      path: '/updatePassword',\r\n      name: '修改密码',\r\n      component: UpdatePassword,\n      meta: {icon:'', title:'updatePassword'}\r\n    }, {\r\n      path: '/pay',\r\n      name: '支付',\r\n      component: pay,\n      meta: {icon:'', title:'pay'}\r\n    }, {\r\n      path: '/center',\r\n      name: '个人信息',\r\n      component: center,\n      meta: {icon:'', title:'center'}\r\n    }\n      ,{\n\tpath: '/examfailrecord',\n        name: '错题本',\n        component: examfailrecord\n      }\n      ,{\n\tpath: '/shangpinrukujilu',\n        name: '商品入库记录',\n        component: shangpinrukujilu\n      }\n      ,{\n\tpath: '/examquestion',\n        name: '试题管理',\n        component: examquestion\n      }\n      ,{\n\tpath: '/shangpinxinxi',\n        name: '商品信息',\n        component: shangpinxinxi\n      }\n      ,{\n\tpath: '/auctionshangpinxinxi',\n        name: '商品信息出价记录',\n        component: auctionshangpinxinxi\n      }\n      ,{\n\tpath: '/storeup',\n        name: '我的收藏管理',\n        component: storeup\n      }\n      ,{\n\tpath: '/shangjia',\n        name: '商家',\n        component: shangjia\n      }\n      ,{\n\tpath: '/discussyonghushangpin',\n        name: '用户商品评论',\n        component: discussyonghushangpin\n      }\n      ,{\n\tpath: '/systemintro',\n        name: '系统简介',\n        component: systemintro\n      }\n      ,{\n\tpath: '/exampaperlist',\n        name: '试卷列表',\n        component: exampaperlist\n      }\n      ,{\n\tpath: '/examrecord',\n        name: '考试记录',\n        component: examrecord\n      }\n      ,{\n\tpath: '/weixiupingjia',\n        name: '维修评价',\n        component: weixiupingjia\n      }\n      ,{\n\tpath: '/news',\n        name: '新闻资讯',\n        component: news\n      }\n      ,{\n\tpath: '/yonghushangpin',\n        name: '用户商品',\n        component: yonghushangpin\n      }\n      ,{\n\tpath: '/aboutus',\n        name: '关于我们',\n        component: aboutus\n      }\n      ,{\n\tpath: '/pingjiaxinxi',\n        name: '评价信息',\n        component: pingjiaxinxi\n      }\n      ,{\n\tpath: '/pynews',\n        name: '爬虫',\n        component: pynews\n      }\n      ,{\n\tpath: '/exampaper',\n        name: '试卷管理',\n        component: exampaper\n      }\n      ,{\n\tpath: '/menu',\n        name: '菜单列表',\n        component: menu\n      }\n      ,{\n\tpath: '/users',\n        name: '管理员',\n        component: users\n      }\n      ,{\n\tpath: '/dingdanjilu',\n        name: '订单记录',\n        component: dingdanjilu\n      }\n      ,{\n\tpath: '/kefangpingjia',\n        name: '客房评价',\n        component: kefangpingjia\n      }\n      ,{\n\tpath: '/shangpinfenlei',\n        name: '商品分类',\n        component: shangpinfenlei\n      }\n      ,{\n\tpath: '/chat',\n        name: '客服咨询',\n        component: chat\n      }\n      ,{\n\tpath: '/discussshangpinxinxi',\n        name: '商品信息评论',\n        component: discussshangpinxinxi\n      }\n      ,{\n\tpath: '/messages',\n        name: '留言板管理',\n        component: messages\n      }\n      ,{\n        path: '/orders/:status',\n        name: '订单管理',\n        component: orders\n      }\n      ,{\n\tpath: '/fahuojilu',\n        name: '发货记录',\n        component: fahuojilu\n      }\n      ,{\n\tpath: '/user',\n        name: '用户',\n        component: user\n      }\n      ,{\n\tpath: '/shangpinchukujilu',\n        name: '商品出库记录',\n        component: shangpinchukujilu\n      }\n    ]\r\n  },\r\n  {\r\n    path: '/adminexam',\r\n    name: 'adminexam',\r\n    component: adminexam,\n    meta: {icon:'', title:'adminexam'}\r\n  },\r\n  {\r\n    path: '/login',\r\n    name: 'login',\r\n    component: Login,\n    meta: {icon:'', title:'login'}\r\n  },\r\n  {\r\n    path: '/board',\r\n    name: 'board',\r\n    component: Board,\n    meta: {icon:'', title:'board'}\r\n  },\r\n  {\r\n    path: '/register',\r\n    name: 'register',\r\n    component: register,\n    meta: {icon:'', title:'register'}\r\n  },\r\n  {\r\n    path: '/',\r\n    name: '首页',\r\n    redirect: '/index'\r\n  }, /*默认跳转路由*/\r\n  {\r\n    path: '*',\r\n    component: NotFound\r\n  }\r\n]\r\n//3.实例化VueRouter  注意：名字\r\nconst router = new VueRouter({\r\n  mode: 'hash',\r\n  /*hash模式改为history*/\r\n  routes // （缩写）相当于 routes: routes\r\n})\r\nconst originalPush = VueRouter.prototype.push\n//修改原型对象中的push方法\nVueRouter.prototype.push = function push(location) {\n   return originalPush.call(this, location).catch(err => err)\n}\nexport default router;\n"]}]}