isplay\":\"flex\"}'>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px 40px\",\"background\":\"#5497f2\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('shangpinfenlei','新增')\" type=\"success\" @click=\"addOrUpdateHandler()\">新增</el-button>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 5px\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px 40px\",\"background\":\"rgba(255, 0, 0, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('shangpinfenlei','删除')\" :disabled=\"dataListSelections.length <= 0\" type=\"danger\" @click=\"deleteHandler()\">删除</el-button>\n\n\n\n\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 5px\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"20px 40px\",\"background\":\"rgba(255, 128, 0, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('shangpinfenlei','报表')\" type=\"warning\" @click=\"chartDialog()\">统计报表</el-button>\n\t\t\t\t</el-row>\n\t\t\t</el-form>\r\n\t\t\t\n\t\t\t<!-- <div> -->\n\t\t\t\t<el-table class=\"tables\"\r\n\t\t\t\t\t:stripe='false'\r\n\t\t\t\t\t:style='{\"padding\":\"0\",\"borderColor\":\"#e5e5e5\",\"borderRadius\":\"20px  20px\",\"borderWidth\":\"1px 0 0 1px\",\"background\":\"#fff\",\"width\":\"100%\",\"borderStyle\":\"solid\"}' \r\n\t\t\t\t\tv-if=\"isAuth('shangpinfenlei','查看')\"\n\t\t\t\t\t:data=\"dataList\"\n\t\t\t\t\tv-loading=\"dataListLoading\"\n\t\t\t\t@selection-change=\"selectionChangeHandler\">\n\t\t\t\t\t<el-table-column :resizable='true' type=\"selection\" align=\"center\" width=\"50\"></el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' label=\"索引\" type=\"index\" width=\"50\" />\n\t\t\t\t\t<el-tabl