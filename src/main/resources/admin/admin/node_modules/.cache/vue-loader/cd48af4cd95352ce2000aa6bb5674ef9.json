\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"dianyingleixing\"\n\t\t\t\t\tlabel=\"电影类型\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.dianyingleixing}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' prop=\"dianyingfengmian\" width=\"200\" label=\"电影封面\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<div v-if=\"scope.row.dianyingfengmian\">\n\t\t\t\t\t\t\t\t<img v-if=\"scope.row.dianyingfengmian.substring(0,4)=='http'\" :src=\"scope.row.dianyingfengmian.split(',')[0]\" width=\"100\" height=\"100\">\n\t\t\t\t\t\t\t\t<img v-else :src=\"$base.url+scope.row.dianyingfengmian.split(',')[0]\" width=\"100\" height=\"100\">\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t\t<div v-else>无图片</div>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"shangyingnianfen\"\n\t\t\t\t\tlabel=\"上映年份\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.shangyingnianfen}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"shangyingriqi\"\n\t\t\t\t\tlabel=\"上映日期\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.shangyingriqi}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' prop=\"dianyingpianduan\" label=\"电影片段\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<el-button v-if=\"scope.row.dianyingpianduan\" type=\"text\" size=\"small\" @click=\"download($base.url+scope.row.dianyingpianduan)\">预览</el-button>\n                            <span v-else >无</span>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"dianyingshizhang\"\n\t\t\t\t\tlabel=\"电影时长\">\n\t