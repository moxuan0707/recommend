t\t\t\t<el-button :style='{\"border\":\"1px solid #22d6c3\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#22d6c3\",\"borderRadius\":\"20px\",\"background\":\"rgba(31,191,174,.1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('dianyingxinxi','修改')\" type=\"primary\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button>\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #c1ccf4\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#c1ccf4\",\"borderRadius\":\"20px\",\"background\":\"rgba(193,204,244,.1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('dianyingxinxi','查看评论')\" type=\"primary\" size=\"mini\" @click=\"disscussListHandler(scope.row.id)\">查看评论</el-button>\n\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"1px solid #dc333b\",\"cursor\":\"pointer\",\"padding\":\"0 16px\",\"margin\":\"3px 6px 3px 0\",\"outline\":\"none\",\"color\":\"#dc333b\",\"borderRadius\":\"20px\",\"background\":\"rgba(220,51,59,.1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('dianyingxinxi','删除') \" type=\"danger\" size=\"mini\" @click=\"deleteHandler(scope.row.id)\">删除</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t</el-table>\n\t\t\t\t<el-pagination\r\n\t\t\t\t\t@size-change=\"sizeChangeHandle\"\n\t\t\t\t\t@current-change=\"currentChangeHandle\"\n\t\t\t\t\t:current-page=\"pageIndex\"\n\t\t\t\t\tbackground\r\n\t\t\t\t\t:page-sizes=\"[10, 20, 30, 50]\"\r\n\t\t\t\t\t:page-size=\"pageSize\"\r\n\t\t\t\t\t:layout=\"layouts.join()\"\r\n\t\t\t\t\t:total=\"totalPage\"\r\n\t\t\t\t\tprev-text=\"<\"\r\n\t\t\t\t\tnext-text=\">\"\r\n\t\t\t\t\t:hide-on-single-page=\"true\"\r\n\t\t\t\t\t:style='{\"padding\":\"8px 10px\",\"margin\":\"20px 0 0\",\"borderColor\":\"#2adbcb\",\"whiteSpace\":\"nowrap\",\"color\":\"#b35ff3\",\"background\":\"none\",\"borderWidth\":\"0px\",\"width\":\"100%\",\"lineHeight\":\"54px\",\"borderStyle\":\"dotted dashed solid double\",\"fontWeight\":\"500\",\"order\":\"5\",\"height\":\"54px\"}'\n\t\t\t\t></el-pagination>\n\t\t\t<!-- </div> -->\n\t\t</template>\r\n\t\t\n\t\t<!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n\t\t<add-or-update v-if=\"addOrUpdateFlag\" :parent=\"this\" ref=\"addOrUpdate\"></add-or-update>\n\n\t\t<dianyingpingfen-cross-add-or-update v-if=\"dianyingpingfenCrossAddOrUpdateFlag\" :parent=\"this\" ref=\"dianyingpingfenCrossaddOrUpdate\"></dianyingpingfen-cross-add-or-update>\n\n\n\n\n\t</div>\n</template>\r\n\n<script>\nimport axios from 'axios'\nimport AddOrUpdate from \"./add-or-update\";\nimport dianyingpingfenCrossAddOrUpdate from \"../dianyingpingfen/add-or-update\";\nexport default {\n  data() {\n    return {\r\n      searchForm: {\n        key: \"\"\n      },\n      form:{},\n      dataList: [],\n      pageIndex: 1,\n      pageSize: 10,\n      totalPage: 0,\n      dataListLoading: false,\n      dataListSelections: [],\n      showFlag: true,\n      sfshVisiable: false,\n      shForm: {},\n      chartVisiable: false,\n      chartVisiable1: false,\n      chartVisiable2: false,\n      chartVisiable3: false,\n      chartVisiable4: false,\n      chartVisiable5: false,\n      addOrUpdateFlag:false,\n      dianyingpingfenCrossAddOrUpdateFlag: false,\n      layouts: [\"total\",\"prev\",\"pager\",\"next\",\"sizes\",\"jumper\"],\n\n\n    };\n  },\n  created() {\n    this.init();\n    this.getDataList();\n    this.contentStyleChange()\r\n  },\n  mounted() {\n  },\n  filters: {\n    htmlfilter: function (val) {\n      return val.replace(/<[^>]*>/g).replace(/undefined/g,'');\n    }\n  },\n  components: {\n    AddOrUpdate,\n    dianyingpingfenCrossAddOrUpdate,\n  },\n  methods: {\n\n    contentStyleChange() {\n      this.contentPageStyleChange()\n    },\n    // 分页\n    contentPageStyleChange(){\n      let arr = []\n\n      // if(this.contents.pageTotal) arr.push(